Problem link:

https://www.geeksforgeeks.org/batch/gfg-160-problems/track/arrays-gfg-160/problem/move-all-zeroes-to-end-of-array0751

class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        // code here
        int count = 0;
        int n = arr.size();
        for(int i =0 ; i<n;i++)
        {
            if(arr[i] != 0)
            {
                swap(arr[i] , arr[count]);
                count++;
            }
        }
    }
};
